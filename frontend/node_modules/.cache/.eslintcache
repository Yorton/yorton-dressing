);\r\n        }else{\r\n\r\n            setName(userInfo.name !== user.name ? userInfo.name : user.name);\r\n            setEmail(userInfo.email !== user.email ? userInfo.email :user.email);\r\n        }\r\n   \r\n    },[dispatch, userInfo._id, user]);\r\n\r\n    const submitHandler = (e) => {\r\n\r\n        e.preventDefault();\r\n\r\n        if (password != confirmPassword){\r\n            alert('Password and ConfrimPassword Are Not Matched');\r\n        }else{\r\n            dispatch(updateUserProfile({userId: user._id, name, email, password}));\r\n        }\r\n    };\r\n\r\n    return (\r\n        <div>\r\n            <form className='form' onSubmit={submitHandler}>\r\n                <div>\r\n                    <h1>User Profile</h1>\r\n                </div>\r\n                {\r\n                    loading ? (<LoadingBox></LoadingBox>) :\r\n                    error ? (<MessageBox variant='danger'>{error}</MessageBox>) :\r\n                    (\r\n                        <>\r\n                            {loadingUpdate && (<LoadingBox></LoadingBox>)}\r\n                            {errorUpdate && (<MessageBox variant='danger'>{errorUpdate}</MessageBox>)}\r\n                            {successUpdate && (<MessageBox variant='success'>Profile Updated Successfully</MessageBox>)}\r\n                            <div>\r\n                                <label htmlFor='name'>Name</label>\r\n                                <input id='name' type='text' placeholder='Enter name' value={name}\r\n                                onChange={(e) => {setName(e.target.value);}}></input>\r\n                            </div>\r\n                            <div>\r\n                                <label htmlFor='email'>Email</label>\r\n                                <input id='email' type='email' placeholder='Enter email' value={email}\r\n                                onChange={(e) => {setEmail(e.target.value);}}></input>\r\n                            </div>\r\n            